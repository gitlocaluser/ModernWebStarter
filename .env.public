# NextAuth.js Core
#NEXTAUTH_URL=http://localhost:3000
#NEXTAUTH_SECRET=your-random-secret  # Generate with `openssl rand -base64 32` or a long random string

# Google Provider
#GOOGLE_CLIENT_ID=your-google-client-id
#GOOGLE_CLIENT_SECRET=your-google-client-secret

# GitHub Provider (placeholders, get from GitHub later)
#GITHUB_CLIENT_ID=your-github-client-id
#GITHUB_CLIENT_SECRET=your-github-client-secret

# Email Provider (placeholders, requires PostgreSQL and email service)
#EMAIL_SERVER=smtp://username:password@smtp.example.com:587
#EMAIL_FROM=noreply@yourdomain.com
#POSTGRES_URL=postgres://postgres:yourpassword@localhost:5432/mywebappdb